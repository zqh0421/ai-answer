    "@types/redux": "^3.6.0",
    "@types/redux-persist": "^4.3.1",
    "axios": "^1.7.7",
    "concurrently": "^9.0.1",
    "framer-motion": "^11.5.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "next": "15.0.0-canary.179",
    "next-auth": "5.0.0-beta.22",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^9.0.1",
    "react-redux": "^8.1.1",
