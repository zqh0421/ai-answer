{
  "name": "ai-answer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "next-dev": "pnpm install && next dev",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "concurrently": "^9.0.1",
    "framer-motion": "^11.5.6",
    "next": "14.2.12",
    "react": "^18",
    "react-dom": "^18",
    "react-dropzone": "^14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.12"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.12",
    "typescript": "^5"
  }
}
