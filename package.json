{
  "name": "ai-answer",
  "version": "1.1.1",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "dev": "concurrently \"pnpm -C frontend dev\" \"pnpm run fastapi-dev\"",
    "fastapi-dev": "./venv/bin/python -m uvicorn api.index:app --reload",
    "dev:frontend": "pnpm -C frontend dev",
    "dev:api": "pnpm -C api fastapi-dev",
    "build": "pnpm -C frontend build",
    "start": "concurrently \"pnpm -C frontend start\" \"pnpm -C api fastapi-gstart\"",
    "lint": "pnpm -C frontend lint",
    "fastapi-gdev": "pnpm -C api fastapi-gdev",
    "fastapi-start": "pnpm -C api fastapi-start",
    "fastapi-gstart": "pnpm -C api fastapi-gstart"
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  },
  "engines": {
    "node": ">=20.17.0"
  }
}
